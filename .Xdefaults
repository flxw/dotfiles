!!!! urxvt behaviour !!!!!!!!!!!!!!!!!!!!!!!!!!!!
URxvt*saveLines:        12000
URxvt*font:		        xft:Inconsolata for Powerline:pixelsize=16

URxvt*scrollBar:        true
URxvt*scrollBar_right:  false
URxvt*scrollstyle:      rxvt
URxvt*cursorUnderline:  false
URxvt*urgentOnBell:     true
URxvt*visualBell:       false

!!!! irssi modifier bindings
URxvt*modifier:         SUPER_L
! open urls in firefox on mouseclick
!URxvt.keysym.M-u: perl:mark-yank-urls:activate_mark_mode
!URxvt.perl-ext: selection,mark-yank-urls,matcher
URxvt.urlLauncher: chromium
!URxvt.matcher.button: 1

!!!! color definitions !!!!!!!!!!!!!!!!!!!!!!!!!!
!URxvt*transparent:          true
!URxvt*.inheritPixmap:   true
URxvt*depth:                32
#URxvt*tintColor:            #1d1f21
! shading - 0 to 99 darkens, 101 to 200 lightens.
URxvt*shading:               20
URxvt*cursorColor:           #93a1a1
URxvt*pointerColorBackground:#586e75
URxvt*pointerColorForeground:#93a1a1
#URxvt*fading:               70
#URxvt.fadeColor:            #1d1f21

