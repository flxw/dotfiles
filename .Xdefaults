!!!! urxvt behaviour !!!!!!!!!!!!!!!!!!!!!!!!!!!!
*saveLines:        12000
*font:		        xft:Inconsolata for Powerline:pixelsize=16

*scrollBar:        true
*scrollBar_right:  false
*scrollstyle:      rxvt
*cursorUnderline:  false
*urgentOnBell:     true
*visualBell:       false

!!!! irssi modifier bindings
*modifier:         SUPER_L
! open urls in firefox on mouseclick
!.keysym.M-u: perl:mark-yank-urls:activate_mark_mode
!.perl-ext: selection,mark-yank-urls,matcher
.urlLauncher: chromium
!.matcher.button: 1

!!!! color definitions !!!!!!!!!!!!!!!!!!!!!!!!!!
!*transparent:          true
!*.inheritPixmap:   true
*depth:                32
#*tintColor:            #1d1f21
! shading - 0 to 99 darkens, 101 to 200 lightens.
*shading:               20
*cursorColor:           #93a1a1
*pointerColorBackground:#586e75
*pointerColorForeground:#93a1a1
#*fading:               70
#.fadeColor:            #1d1f21

